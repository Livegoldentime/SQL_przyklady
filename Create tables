/*Customers*/
CREATE TABLE Customers (CustomerID int, CustomerName varchar(50), ContactName varchar(25), Address varchar(50), City varchar(25),
PostalCode varchar(25), Country varchar(25));

/*Categories*/
CREATE TABLE Categories (CategoryID int, CategoryName varchar(25), Description varchar(100));

/*Employees*/
CREATE TABLE Employees (EmployessID int, LastName varchar(100), FirstName varchar(100), BirthDate varchar(25), Photo char(100), Notes varchar(1500));

/*OrderDetails*/
CREATE TABLE OrderDetails (OrderDetailID int, OrderID int, ProductID int, Quantity int);

/*Orders*/
CREATE TABLE Orders (OrderID int, CustomerID int, EmployeeID int, OrderDate varchar(25), ShipperID int);

/*Products*/
CREATE TABLE Products (ProductID int, ProductName varchar(50), SupplierID int, CategoryID int, Unit varchar(100), Price int);

/*Shippers*/
CREATE TABLE Shippers (ShipperID int, ShipperName varchar(25), Phone varchar(25))

/*Suppliers*/
CREATE TABLE Suppliers (SupplierID int, SupplierName varchar(100), ContactName varchar(40), Address varchar(100), City varchar(25), PostalCode varchar(25), Country varchar(25), Phone varchar(25))
